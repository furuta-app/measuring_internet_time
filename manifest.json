{
  "name": "measuring_internet_time",
  "manifest_version": 2,
  "version": "1.0",
  "description": "今日のネット時間をアイコン下に表示する",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "browser_action": {
    "default_icon": "icons/icon19.png"
  },
  "permissions": [
    "history"
  ],

  "content_scripts": [
  ],

  "background": {
    "scripts": [
      "moment.js",
      "background.js"
    ]
  }
}
